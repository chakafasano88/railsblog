<!-- This returns a form to create a new User  -->
<div class="container">
  <div class="new_user_form">
<%= form_for @user do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>
  <%= form.label :password %>
  <%= form.text_field :password  %>
  <%= form.label :fname %>
  <%= form.text_field :fname %>
  <%= form.label :lname %>
  <%= form.text_field :lname %>

  <%= form.submit %>
<% end %>
  </div>
</div>
